@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

<http://example.org/Book> a owl:Class ;
    rdfs:label "Book" ;
    rdfs:comment "A written or printed work consisting of pages" .

<http://example.org/author> a owl:ObjectProperty ;
    rdfs:domain <http://example.org/Book> ;
    rdfs:range <http://example.org/Person> .