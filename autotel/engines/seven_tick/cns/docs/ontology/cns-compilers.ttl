@prefix cns: <https://schema.chatman.ai/cns#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# CNS Compilers Ontology - All Compiler and Code Generation Tools
#################################################################

# ============================================================================
# COMPILER CLASSES
# ============================================================================

cns:Compiler a rdfs:Class ;
    rdfs:label "Compiler" ;
    rdfs:comment "A code generation or compilation tool" .

cns:CodeGenerator a rdfs:Class ;
    rdfs:label "Code Generator" ;
    rdfs:comment "A tool that generates code from specifications" .

cns:Template a rdfs:Class ;
    rdfs:label "Template" ;
    rdfs:comment "A template for code generation" .

cns:Language a rdfs:Class ;
    rdfs:label "Programming Language" ;
    rdfs:comment "A programming language supported by compilers" .

# ============================================================================
# COMPILER PROPERTIES
# ============================================================================

cns:generatesCode a rdf:Property ;
    rdfs:label "generates code" ;
    rdfs:domain cns:Compiler ;
    rdfs:range rdfs:Literal .

cns:supportsLanguage a rdf:Property ;
    rdfs:label "supports language" ;
    rdfs:domain cns:Compiler ;
    rdfs:range cns:Language .

cns:usesTemplate a rdf:Property ;
    rdfs:label "uses template" ;
    rdfs:domain cns:Compiler ;
    rdfs:range cns:Template .

cns:filePath a rdf:Property ;
    rdfs:label "file path" ;
    rdfs:domain cns:Compiler ;
    rdfs:range rdfs:Literal .

cns:compilationTime a rdf:Property ;
    rdfs:label "compilation time" ;
    rdfs:domain cns:Compiler ;
    rdfs:range xsd:integer .

cns:outputFormat a rdf:Property ;
    rdfs:label "output format" ;
    rdfs:domain cns:Compiler ;
    rdfs:range rdfs:Literal .

# ============================================================================
# PROGRAMMING LANGUAGES
# ============================================================================

cns:CLanguage a cns:Language ;
    rdfs:label "C Language" ;
    rdfs:comment "C programming language" .

cns:CPPLanguage a cns:Language ;
    rdfs:label "C++ Language" ;
    rdfs:comment "C++ programming language" .

cns:PythonLanguage a cns:Language ;
    rdfs:label "Python Language" ;
    rdfs:comment "Python programming language" .

cns:TTLLanguage a cns:Language ;
    rdfs:label "Turtle Language" ;
    rdfs:comment "Turtle RDF serialization format" .

cns:JSONLanguage a cns:Language ;
    rdfs:label "JSON Language" ;
    rdfs:comment "JSON data format" .

# ============================================================================
# TEMPLATES
# ============================================================================

cns:OTELHeaderTemplate a cns:Template ;
    rdfs:label "OpenTelemetry Header Template" ;
    cns:filePath "cns/templates/otel_header.h.j2" ;
    rdfs:comment "Jinja template for OpenTelemetry headers" .

cns:OTELInjectTemplate a cns:Template ;
    rdfs:label "OpenTelemetry Injection Template" ;
    cns:filePath "cns/templates/otel_inject.c.j2" ;
    rdfs:comment "Jinja template for OpenTelemetry injection" .

cns:ReportTemplate a cns:Template ;
    rdfs:label "Report Generation Template" ;
    cns:filePath "cns/templates/report.j2" ;
    rdfs:comment "Jinja template for report generation" .

cns:SimpleTemplate a cns:Template ;
    rdfs:label "Simple Template" ;
    cns:filePath "cns/templates/simple.j2" ;
    rdfs:comment "Simple Jinja template example" .

