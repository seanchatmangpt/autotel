    cns:supportsLanguage cns:TTLLanguage ;
    cns:supportsLanguage cns:JSONLanguage .

cns:SpanValidator cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:JSONLanguage .

# Report Generator Relationships
