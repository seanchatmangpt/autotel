# ============================================================================
# COMPILER RELATIONSHIPS
# ============================================================================

# SQL Compiler Relationships
cns:SQLCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

cns:SQLAOTCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

# SPARQL Compiler Relationships
cns:SPARQLCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

cns:SPARQL80_20Compiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

# SHACL Compiler Relationships
cns:SHACLCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

cns:SHACL49CycleCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

# OWL Compiler Relationships
cns:OWLCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

cns:OWL80_20Compiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CLanguage .

# Weaver Compiler Relationships
cns:WeaverCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CPPLanguage ;
    cns:usesTemplate cns:OTELHeaderTemplate ;
    cns:usesTemplate cns:OTELInjectTemplate .

cns:WeaverSimpleCompiler cns:supportsLanguage cns:PythonLanguage ;
    cns:supportsLanguage cns:CPPLanguage .

cns:WeaverBenchmarkCompiler cns:supportsLanguage cns:PythonLanguage ;
