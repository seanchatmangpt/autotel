    cns:targetPlatform "multi" ;
    rdfs:comment "Demo execution automation" .

# ============================================================================
# BUILD RELATIONSHIPS
# ============================================================================

# OWL Build System Relationships
cns:OWLBuildSystem cns:buildsComponent cns:OWLEngine ;
    cns:usesScript cns:TestAutomationScript ;
    cns:usesScript cns:BenchmarkAutomationScript .

# SHACL Build System Relationships
cns:SHACLBuildSystem cns:buildsComponent cns:SHACLEngine ;
    cns:usesScript cns:TestAutomationScript ;
    cns:usesScript cns:BenchmarkAutomationScript .

# Benchmark Build System Relationships
cns:BenchmarkBuildSystem cns:buildsComponent cns:BenchmarkEngine ;
    cns:usesScript cns:BenchmarkAutomationScript ;
    cns:usesScript cns:PerformanceValidationScript .

# Test Build System Relationships
cns:TestBuildSystem cns:usesScript cns:TestAutomationScript .

# ARM64 Build System Relationships
cns:ARM64BuildSystem cns:usesScript cns:TestAutomationScript ;
    cns:usesScript cns:BenchmarkAutomationScript .

# Main Makefile Relationships
cns:MainMakefile cns:buildsComponent cns:CNSEngine ;
    cns:buildsComponent cns:SPARQLEngine ;
    cns:buildsComponent cns:SHACLEngine ;
    cns:buildsComponent cns:OWLEngine ;
    cns:buildsComponent cns:BenchmarkEngine ;
    cns:usesScript cns:TestAutomationScript ;
    cns:usesScript cns:BenchmarkAutomationScript ;
    cns:usesScript cns:PerformanceValidationScript .

# CMake Relationships
cns:CMakeLists cns:buildsComponent cns:CNSEngine ;
    cns:buildsComponent cns:SPARQLEngine ;
    cns:buildsComponent cns:SHACLEngine ;
    cns:buildsComponent cns:OWLEngine ;
    cns:buildsComponent cns:BenchmarkEngine ;
    cns:usesScript cns:TestAutomationScript ;
    cns:usesScript cns:BenchmarkAutomationScript . 