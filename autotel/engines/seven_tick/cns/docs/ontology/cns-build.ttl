# ============================================================================
# AUTOMATION SCRIPTS
# ============================================================================

cns:BenchmarkAutomationScript a cns:Script ;
    rdfs:label "Benchmark Automation Script" ;
    cns:filePath "cns/run_comprehensive_benchmarks.sh" ;
    cns:buildTime 300 ;
    cns:targetPlatform "multi" ;
    cns:usesScript cns:PerformanceValidationScript ;
    rdfs:comment "Automated benchmark execution" .

cns:TestAutomationScript a cns:Script ;
    rdfs:label "Test Automation Script" ;
    cns:filePath "cns/tests/run_tests.sh" ;
    cns:buildTime 120 ;
