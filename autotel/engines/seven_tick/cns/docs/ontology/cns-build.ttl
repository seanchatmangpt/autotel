@prefix cns: <https://schema.chatman.ai/cns#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

#################################################################
# CNS Build Ontology - All Build Systems and Automation Tools
#################################################################

# ============================================================================
# BUILD CLASSES
# ============================================================================

cns:BuildSystem a rdfs:Class ;
    rdfs:label "Build System" ;
    rdfs:comment "A build system for CNS components" .

cns:Makefile a rdfs:Class ;
    rdfs:label "Makefile" ;
    rdfs:comment "A Makefile for building CNS components" .

cns:Script a rdfs:Class ;
    rdfs:label "Script" ;
    rdfs:comment "A build or automation script" .

cns:Configuration a rdfs:Class ;
    rdfs:label "Configuration" ;
    rdfs:comment "A build configuration file" .

cns:Target a rdfs:Class ;
    rdfs:label "Build Target" ;
    rdfs:comment "A build target or goal" .

# ============================================================================
# BUILD PROPERTIES
# ============================================================================

cns:buildsComponent a rdf:Property ;
    rdfs:label "builds component" ;
    rdfs:domain cns:BuildSystem ;
    rdfs:range cns:Engine .

cns:hasTarget a rdf:Property ;
    rdfs:label "has target" ;
    rdfs:domain cns:BuildSystem ;
    rdfs:range cns:Target .

cns:usesScript a rdf:Property ;
    rdfs:label "uses script" ;
    rdfs:domain cns:BuildSystem ;
    rdfs:range cns:Script .

cns:filePath a rdf:Property ;
    rdfs:label "file path" ;
    rdfs:domain cns:BuildSystem ;
    rdfs:range rdfs:Literal .

cns:buildTime a rdf:Property ;
    rdfs:label "build time" ;
    rdfs:domain cns:BuildSystem ;
    rdfs:range xsd:integer .

cns:targetPlatform a rdf:Property ;
    rdfs:label "target platform" ;
    rdfs:domain cns:BuildSystem ;
    rdfs:range rdfs:Literal .

cns:optimizationLevel a rdf:Property ;
    rdfs:label "optimization level" ;
    rdfs:domain cns:BuildSystem ;
    rdfs:range rdfs:Literal .

# ============================================================================
# MAKEFILES
# ============================================================================

cns:MainMakefile a cns:Makefile ;
    rdfs:label "Main Makefile" ;
    cns:filePath "cns/Makefile" ;
    cns:buildTime 120 ;
    cns:targetPlatform "multi" ;
    cns:optimizationLevel "O2" ;
    rdfs:comment "Main CNS build system" .

