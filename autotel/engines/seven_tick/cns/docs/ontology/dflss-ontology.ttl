@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix cns: <http://cns.autotel.org/ontology#> .
@prefix dflss: <http://cns.autotel.org/ontology/dflss#> .
@prefix perf: <http://cns.autotel.org/ontology/performance#> .
@prefix telemetry: <http://cns.autotel.org/ontology/telemetry#> .

# Design for Lean Six Sigma Ontology
# Focuses on 7-tick performance constraints and CNS integration

# =============================================================================
# CORE DFLSS CONCEPTS
# =============================================================================

# DFLSS Process
dflss:DFLSSProcess a owl:Class ;
    rdfs:label "DFLSS Process" ;
    rdfs:comment "A complete Design for Lean Six Sigma process with 7-tick performance constraints" ;
    rdfs:subClassOf cns:Process .

# DFLSS Phases
dflss:DFLSSPhase a owl:Class ;
    rdfs:label "DFLSS Phase" ;
    rdfs:comment "A phase within the DFLSS methodology" ;
    rdfs:subClassOf cns:ProcessPhase .

# Define (D) Phase
dflss:DefinePhase a owl:Class ;
    rdfs:label "Define Phase" ;
    rdfs:comment "Define phase: project scope, customer requirements, and performance targets" ;
    rdfs:subClassOf dflss:DFLSSPhase ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "1024"^^xsd:integer .

# Measure (M) Phase  
dflss:MeasurePhase a owl:Class ;
    rdfs:label "Measure Phase" ;
    rdfs:comment "Measure phase: data collection, measurement system analysis, and baseline performance" ;
    rdfs:subClassOf dflss:DFLSSPhase ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "2048"^^xsd:integer .

# Analyze (A) Phase
dflss:AnalyzePhase a owl:Class ;
    rdfs:label "Analyze Phase" ;
    rdfs:comment "Analyze phase: root cause analysis, statistical analysis, and performance modeling" ;
    rdfs:subClassOf dflss:DFLSSPhase ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "4096"^^xsd:integer .

# Design (D) Phase
dflss:DesignPhase a owl:Class ;
    rdfs:label "Design Phase" ;
    rdfs:comment "Design phase: solution design, optimization, and performance validation" ;
    rdfs:subClassOf dflss:DFLSSPhase ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "8192"^^xsd:integer .

# Verify (V) Phase
dflss:VerifyPhase a owl:Class ;
    rdfs:label "Verify Phase" ;
    rdfs:comment "Verify phase: implementation verification, performance testing, and validation" ;
    rdfs:subClassOf dflss:DFLSSPhase ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "4096"^^xsd:integer .

# =============================================================================
# LEAN CONCEPTS
# =============================================================================

# Lean Principles
dflss:LeanPrinciple a owl:Class ;
    rdfs:label "Lean Principle" ;
    rdfs:comment "Core lean principles for waste elimination and value optimization" ;
    rdfs:subClassOf cns:Principle .

# Value
dflss:Value a owl:Class ;
    rdfs:label "Value" ;
    rdfs:comment "What the customer is willing to pay for" ;
    rdfs:subClassOf dflss:LeanPrinciple ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "128"^^xsd:integer .

# Value Stream
dflss:ValueStream a owl:Class ;
    rdfs:label "Value Stream" ;
    rdfs:comment "The sequence of activities that deliver value to the customer" ;
    rdfs:subClassOf dflss:LeanPrinciple ;
    perf:maxExecutionTime "3"^^xsd:integer ;
    perf:maxMemoryUsage "512"^^xsd:integer .

# Flow
dflss:Flow a owl:Class ;
    rdfs:label "Flow" ;
    rdfs:comment "Smooth, uninterrupted movement of work through the system" ;
    rdfs:subClassOf dflss:LeanPrinciple ;
    perf:maxExecutionTime "2"^^xsd:integer ;
    perf:maxMemoryUsage "256"^^xsd:integer .

# Pull
dflss:Pull a owl:Class ;
    rdfs:label "Pull" ;
    rdfs:comment "Producing only what is needed when it is needed" ;
    rdfs:subClassOf dflss:LeanPrinciple ;
    perf:maxExecutionTime "2"^^xsd:integer ;
    perf:maxMemoryUsage "256"^^xsd:integer .

# Perfection
dflss:Perfection a owl:Class ;
    rdfs:label "Perfection" ;
    rdfs:comment "Continuous improvement toward perfect value with zero waste" ;
    rdfs:subClassOf dflss:LeanPrinciple ;
    perf:maxExecutionTime "5"^^xsd:integer ;
    perf:maxMemoryUsage "1024"^^xsd:integer .

# Waste Types
dflss:Waste a owl:Class ;
    rdfs:label "Waste" ;
    rdfs:comment "Any activity that does not add value to the customer" ;
    rdfs:subClassOf cns:Problem .

# 8 Types of Waste
dflss:Defects a owl:Class ;
    rdfs:label "Defects" ;
    rdfs:comment "Work that contains errors or requires rework" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

dflss:Overproduction a owl:Class ;
    rdfs:label "Overproduction" ;
    rdfs:comment "Producing more than what is needed or before it is needed" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

dflss:Waiting a owl:Class ;
    rdfs:label "Waiting" ;
    rdfs:comment "Idle time when resources are not being utilized" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

dflss:NonUtilizedTalent a owl:Class ;
    rdfs:label "Non-Utilized Talent" ;
    rdfs:comment "Underutilizing people's skills, knowledge, and abilities" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

dflss:Transportation a owl:Class ;
    rdfs:label "Transportation" ;
    rdfs:comment "Unnecessary movement of materials or information" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

dflss:Inventory a owl:Class ;
    rdfs:label "Inventory" ;
    rdfs:comment "Excess materials, work-in-process, or finished goods" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

dflss:Motion a owl:Class ;
    rdfs:label "Motion" ;
    rdfs:comment "Unnecessary movement of people or equipment" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

dflss:ExtraProcessing a owl:Class ;
    rdfs:label "Extra Processing" ;
    rdfs:comment "Unnecessary steps or processing that adds no value" ;
    rdfs:subClassOf dflss:Waste ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "64"^^xsd:integer .

# =============================================================================
# SIX SIGMA CONCEPTS
# =============================================================================

# Six Sigma Level
dflss:SixSigmaLevel a owl:Class ;
    rdfs:label "Six Sigma Level" ;
    rdfs:comment "Performance level measured in defects per million opportunities" ;
    rdfs:subClassOf cns:PerformanceMetric .

# Sigma Levels
dflss:OneSigma a owl:Class ;
    rdfs:label "One Sigma" ;
    rdfs:comment "690,000 DPMO (30.85% yield)" ;
    rdfs:subClassOf dflss:SixSigmaLevel ;
    dflss:defectsPerMillion "690000"^^xsd:integer ;
    dflss:yieldPercentage "30.85"^^xsd:decimal .

dflss:TwoSigma a owl:Class ;
    rdfs:label "Two Sigma" ;
    rdfs:comment "308,000 DPMO (69.15% yield)" ;
    rdfs:subClassOf dflss:SixSigmaLevel ;
    dflss:defectsPerMillion "308000"^^xsd:integer ;
    dflss:yieldPercentage "69.15"^^xsd:decimal .

dflss:ThreeSigma a owl:Class ;
    rdfs:label "Three Sigma" ;
    rdfs:comment "66,800 DPMO (93.32% yield)" ;
    rdfs:subClassOf dflss:SixSigmaLevel ;
    dflss:defectsPerMillion "66800"^^xsd:integer ;
    dflss:yieldPercentage "93.32"^^xsd:decimal .

dflss:FourSigma a owl:Class ;
    rdfs:label "Four Sigma" ;
    rdfs:comment "6,210 DPMO (99.38% yield)" ;
    rdfs:subClassOf dflss:SixSigmaLevel ;
    dflss:defectsPerMillion "6210"^^xsd:integer ;
    dflss:yieldPercentage "99.38"^^xsd:decimal .

dflss:FiveSigma a owl:Class ;
    rdfs:label "Five Sigma" ;
    rdfs:comment "233 DPMO (99.98% yield)" ;
    rdfs:subClassOf dflss:SixSigmaLevel ;
    dflss:defectsPerMillion "233"^^xsd:integer ;
    dflss:yieldPercentage "99.98"^^xsd:decimal .

dflss:SixSigma a owl:Class ;
    rdfs:label "Six Sigma" ;
    rdfs:comment "3.4 DPMO (99.9997% yield)" ;
    rdfs:subClassOf dflss:SixSigmaLevel ;
    dflss:defectsPerMillion "3"^^xsd:integer ;
    dflss:yieldPercentage "99.9997"^^xsd:decimal .

# Statistical Tools
dflss:StatisticalTool a owl:Class ;
    rdfs:label "Statistical Tool" ;
    rdfs:comment "Tools for statistical analysis and process improvement" ;
    rdfs:subClassOf cns:Tool .

# Common Statistical Tools
dflss:ControlChart a owl:Class ;
    rdfs:label "Control Chart" ;
    rdfs:comment "Statistical process control chart for monitoring process stability" ;
    rdfs:subClassOf dflss:StatisticalTool ;
    perf:maxExecutionTime "5"^^xsd:integer ;
    perf:maxMemoryUsage "2048"^^xsd:integer .

dflss:Histogram a owl:Class ;
    rdfs:label "Histogram" ;
    rdfs:comment "Frequency distribution chart for data analysis" ;
    rdfs:subClassOf dflss:StatisticalTool ;
    perf:maxExecutionTime "3"^^xsd:integer ;
    perf:maxMemoryUsage "1024"^^xsd:integer .

dflss:ParetoChart a owl:Class ;
    rdfs:label "Pareto Chart" ;
    rdfs:comment "80/20 analysis chart for identifying vital few factors" ;
    rdfs:subClassOf dflss:StatisticalTool ;
    perf:maxExecutionTime "4"^^xsd:integer ;
    perf:maxMemoryUsage "1536"^^xsd:integer .

dflss:ScatterPlot a owl:Class ;
    rdfs:label "Scatter Plot" ;
    rdfs:comment "Correlation analysis between two variables" ;
    rdfs:subClassOf dflss:StatisticalTool ;
    perf:maxExecutionTime "3"^^xsd:integer ;
    perf:maxMemoryUsage "1024"^^xsd:integer .

dflss:DesignOfExperiments a owl:Class ;
    rdfs:label "Design of Experiments" ;
    rdfs:comment "Systematic approach to experimentation for optimization" ;
    rdfs:subClassOf dflss:StatisticalTool ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "8192"^^xsd:integer .

# =============================================================================
# DESIGN CONCEPTS
# =============================================================================

# Design Principles
dflss:DesignPrinciple a owl:Class ;
    rdfs:label "Design Principle" ;
    rdfs:comment "Principles for effective design in DFLSS context" ;
    rdfs:subClassOf cns:Principle .

# Robust Design
dflss:RobustDesign a owl:Class ;
    rdfs:label "Robust Design" ;
    rdfs:comment "Design that performs consistently under varying conditions" ;
    rdfs:subClassOf dflss:DesignPrinciple ;
    perf:maxExecutionTime "5"^^xsd:integer ;
    perf:maxMemoryUsage "2048"^^xsd:integer .

# Design for Manufacturability
dflss:DesignForManufacturability a owl:Class ;
    rdfs:label "Design for Manufacturability" ;
    rdfs:comment "Design optimized for efficient manufacturing processes" ;
    rdfs:subClassOf dflss:DesignPrinciple ;
    perf:maxExecutionTime "4"^^xsd:integer ;
    perf:maxMemoryUsage "1536"^^xsd:integer .

# Design for Assembly
dflss:DesignForAssembly a owl:Class ;
    rdfs:label "Design for Assembly" ;
    rdfs:comment "Design optimized for efficient assembly processes" ;
    rdfs:subClassOf dflss:DesignPrinciple ;
    perf:maxExecutionTime "4"^^xsd:integer ;
    perf:maxMemoryUsage "1536"^^xsd:integer .

# Design for Testability
dflss:DesignForTestability a owl:Class ;
    rdfs:label "Design for Testability" ;
    rdfs:comment "Design that facilitates effective testing and validation" ;
    rdfs:subClassOf dflss:DesignPrinciple ;
    perf:maxExecutionTime "3"^^xsd:integer ;
    perf:maxMemoryUsage "1024"^^xsd:integer .

# Design for Reliability
dflss:DesignForReliability a owl:Class ;
    rdfs:label "Design for Reliability" ;
    rdfs:comment "Design that ensures long-term reliability and performance" ;
