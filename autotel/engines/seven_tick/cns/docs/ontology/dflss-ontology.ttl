    perf:maxExecutionTime "5"^^xsd:integer ;
    perf:maxMemoryUsage "2048"^^xsd:integer .

# =============================================================================
# PERFORMANCE METRICS
# =============================================================================

# DFLSS Performance Metrics
dflss:DFLSSMetric a owl:Class ;
    rdfs:label "DFLSS Performance Metric" ;
    rdfs:comment "Performance metrics specific to DFLSS processes" ;
    rdfs:subClassOf cns:PerformanceMetric .

# Process Capability
dflss:ProcessCapability a owl:Class ;
    rdfs:label "Process Capability" ;
    rdfs:comment "Ability of a process to meet specifications consistently" ;
    rdfs:subClassOf dflss:DFLSSMetric ;
    perf:maxExecutionTime "5"^^xsd:integer ;
    perf:maxMemoryUsage "2048"^^xsd:integer .

# Cycle Time
dflss:CycleTime a owl:Class ;
    rdfs:label "Cycle Time" ;
    rdfs:comment "Time required to complete one cycle of a process" ;
    rdfs:subClassOf dflss:DFLSSMetric ;
    perf:maxExecutionTime "1"^^xsd:integer ;
    perf:maxMemoryUsage "128"^^xsd:integer .

# Lead Time
dflss:LeadTime a owl:Class ;
    rdfs:label "Lead Time" ;
    rdfs:comment "Total time from order to delivery" ;
    rdfs:subClassOf dflss:DFLSSMetric ;
    perf:maxExecutionTime "2"^^xsd:integer ;
    perf:maxMemoryUsage "256"^^xsd:integer .

# Takt Time
dflss:TaktTime a owl:Class ;
    rdfs:label "Takt Time" ;
    rdfs:comment "Rate at which products must be produced to meet customer demand" ;
    rdfs:subClassOf dflss:DFLSSMetric ;
    perf:maxExecutionTime "2"^^xsd:integer ;
    perf:maxMemoryUsage "256"^^xsd:integer .

# First Pass Yield
dflss:FirstPassYield a owl:Class ;
    rdfs:label "First Pass Yield" ;
    rdfs:comment "Percentage of products that pass quality inspection on first attempt" ;
    rdfs:subClassOf dflss:DFLSSMetric ;
    perf:maxExecutionTime "2"^^xsd:integer ;
    perf:maxMemoryUsage "256"^^xsd:integer .

# =============================================================================
# TOOLS AND TECHNIQUES
# =============================================================================

# DFLSS Tools
dflss:DFLSSTool a owl:Class ;
    rdfs:label "DFLSS Tool" ;
    rdfs:comment "Tools and techniques used in DFLSS methodology" ;
    rdfs:subClassOf cns:Tool .

# Voice of Customer
dflss:VoiceOfCustomer a owl:Class ;
    rdfs:label "Voice of Customer" ;
    rdfs:comment "Process for capturing and analyzing customer requirements" ;
    rdfs:subClassOf dflss:DFLSSTool ;
    perf:maxExecutionTime "5"^^xsd:integer ;
    perf:maxMemoryUsage "2048"^^xsd:integer .

# Quality Function Deployment
dflss:QualityFunctionDeployment a owl:Class ;
    rdfs:label "Quality Function Deployment" ;
    rdfs:comment "House of Quality matrix for translating customer needs to technical requirements" ;
    rdfs:subClassOf dflss:DFLSSTool ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "4096"^^xsd:integer .

# Failure Mode and Effects Analysis
dflss:FailureModeAndEffectsAnalysis a owl:Class ;
    rdfs:label "Failure Mode and Effects Analysis" ;
    rdfs:comment "Systematic approach for identifying potential failures and their effects" ;
    rdfs:subClassOf dflss:DFLSSTool ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "8192"^^xsd:integer .

# Poka-Yoke
dflss:PokaYoke a owl:Class ;
    rdfs:label "Poka-Yoke" ;
    rdfs:comment "Mistake-proofing devices and procedures" ;
    rdfs:subClassOf dflss:DFLSSTool ;
    perf:maxExecutionTime "3"^^xsd:integer ;
    perf:maxMemoryUsage "1024"^^xsd:integer .

# 5S Methodology
dflss:FiveS a owl:Class ;
    rdfs:label "5S Methodology" ;
    rdfs:comment "Sort, Set in Order, Shine, Standardize, Sustain" ;
    rdfs:subClassOf dflss:DFLSSTool ;
    perf:maxExecutionTime "4"^^xsd:integer ;
    perf:maxMemoryUsage "1536"^^xsd:integer .

# =============================================================================
# RELATIONSHIPS
# =============================================================================

# Phase Relationships
dflss:hasPhase a owl:ObjectProperty ;
    rdfs:label "has phase" ;
    rdfs:domain dflss:DFLSSProcess ;
    rdfs:range dflss:DFLSSPhase .

dflss:precedesPhase a owl:ObjectProperty ;
    rdfs:label "precedes phase" ;
    rdfs:domain dflss:DFLSSPhase ;
    rdfs:range dflss:DFLSSPhase .

# Tool Relationships
dflss:usesTool a owl:ObjectProperty ;
    rdfs:label "uses tool" ;
    rdfs:domain dflss:DFLSSPhase ;
    rdfs:range dflss:DFLSSTool .

dflss:implementsPrinciple a owl:ObjectProperty ;
    rdfs:label "implements principle" ;
    rdfs:domain dflss:DFLSSTool ;
    rdfs:range dflss:LeanPrinciple .

# Metric Relationships
dflss:measuresMetric a owl:ObjectProperty ;
    rdfs:label "measures metric" ;
    rdfs:domain dflss:DFLSSTool ;
    rdfs:range dflss:DFLSSMetric .

dflss:targetsSigmaLevel a owl:ObjectProperty ;
    rdfs:label "targets sigma level" ;
    rdfs:domain dflss:DFLSSProcess ;
    rdfs:range dflss:SixSigmaLevel .

# Waste Relationships
dflss:eliminatesWaste a owl:ObjectProperty ;
    rdfs:label "eliminates waste" ;
    rdfs:domain dflss:LeanPrinciple ;
    rdfs:range dflss:Waste .

# Performance Relationships
dflss:hasPerformanceConstraint a owl:ObjectProperty ;
    rdfs:label "has performance constraint" ;
    rdfs:domain dflss:DFLSSProcess ;
    rdfs:range perf:PerformanceConstraint .

# =============================================================================
# INSTANCES
# =============================================================================

# DFLSS Process Instance
dflss:StandardDFLSSProcess a dflss:DFLSSProcess ;
    rdfs:label "Standard DFLSS Process" ;
    dflss:hasPhase dflss:DefinePhase ;
    dflss:hasPhase dflss:MeasurePhase ;
    dflss:hasPhase dflss:AnalyzePhase ;
    dflss:hasPhase dflss:DesignPhase ;
    dflss:hasPhase dflss:VerifyPhase ;
    dflss:targetsSigmaLevel dflss:SixSigma ;
    perf:maxExecutionTime "7"^^xsd:integer ;
    perf:maxMemoryUsage "16384"^^xsd:integer .

# Phase Sequence
dflss:DefinePhase dflss:precedesPhase dflss:MeasurePhase .
dflss:MeasurePhase dflss:precedesPhase dflss:AnalyzePhase .
dflss:AnalyzePhase dflss:precedesPhase dflss:DesignPhase .
dflss:DesignPhase dflss:precedesPhase dflss:VerifyPhase .

# Tool Assignments
dflss:DefinePhase dflss:usesTool dflss:VoiceOfCustomer .
dflss:DefinePhase dflss:usesTool dflss:QualityFunctionDeployment .
dflss:MeasurePhase dflss:usesTool dflss:ControlChart .
dflss:MeasurePhase dflss:usesTool dflss:Histogram .
dflss:AnalyzePhase dflss:usesTool dflss:ParetoChart .
dflss:AnalyzePhase dflss:usesTool dflss:ScatterPlot .
dflss:DesignPhase dflss:usesTool dflss:DesignOfExperiments .
dflss:DesignPhase dflss:usesTool dflss:FailureModeAndEffectsAnalysis .
dflss:VerifyPhase dflss:usesTool dflss:PokaYoke .
dflss:VerifyPhase dflss:usesTool dflss:FiveS .

# Principle Implementations
dflss:Value dflss:eliminatesWaste dflss:Overproduction .
dflss:Value dflss:eliminatesWaste dflss:ExtraProcessing .
dflss:Flow dflss:eliminatesWaste dflss:Waiting .
dflss:Flow dflss:eliminatesWaste dflss:Transportation .
dflss:Pull dflss:eliminatesWaste dflss:Inventory .
dflss:Perfection dflss:eliminatesWaste dflss:Defects .

# Metric Measurements
dflss:ControlChart dflss:measuresMetric dflss:ProcessCapability .
dflss:Histogram dflss:measuresMetric dflss:FirstPassYield .
dflss:ScatterPlot dflss:measuresMetric dflss:CycleTime .
dflss:DesignOfExperiments dflss:measuresMetric dflss:LeadTime .

# Performance Constraints
dflss:StandardDFLSSProcess dflss:hasPerformanceConstraint perf:SevenTickConstraint .
perf:SevenTickConstraint a perf:PerformanceConstraint ;
    rdfs:label "7-Tick Performance Constraint" ;
    perf:maxCycles "7"^^xsd:integer ;
    perf:maxMemoryBytes "16384"^^xsd:integer ;
    perf:maxLatencyNs "10"^^xsd:integer . 