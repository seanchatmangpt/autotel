# ============================================================================
# TEST RELATIONSHIPS
# ============================================================================

# OWL Engine Test Relationships
cns:OWLEngine cns:hasTest cns:OWLTestSuite ;
    cns:hasTest cns:OWLUnitTests ;
    cns:hasTest cns:OWLPerformanceTests ;
    cns:hasTest cns:OWL7TComplianceTests ;
    cns:hasBenchmark cns:OWLBenchmark .

# Gatekeeper Engine Test Relationships
cns:GatekeeperEngine cns:hasTest cns:GatekeeperTestSuite ;
    cns:hasTest cns:GatekeeperCorrectnessTests ;
    cns:hasTest cns:GatekeeperPerformanceTests ;
    cns:hasTest cns:GatekeeperSigmaTests ;
    cns:hasBenchmark cns:GatekeeperBenchmark .

# SQL AOT Engine Test Relationships
cns:SQLAOTEngine cns:hasTest cns:SQLAOTTestSuite ;
    cns:hasTest cns:SQLAOTCompilationTests ;
    cns:hasTest cns:SQLAOTExecutionTests ;
    cns:hasBenchmark cns:SQLAOTBenchmark ;
    cns:hasBenchmark cns:SQLAOT80_20Benchmark .

# SPARQL Engine Test Relationships
cns:SPARQLEngine cns:hasTest cns:SPARQLTestSuite ;
    cns:hasTest cns:SPARQLQueryTests ;
    cns:hasTest cns:SPARQLJoinTests ;
    cns:hasBenchmark cns:SPARQLBenchmark ;
    cns:hasBenchmark cns:SPARQL80_20Benchmark .

# SHACL Engine Test Relationships
cns:SHACLEngine cns:hasTest cns:SHACLTestSuite ;
    cns:hasTest cns:SHACLValidationTests ;
    cns:hasTest cns:SHACLShapeTests ;
    cns:hasBenchmark cns:SHACLBenchmark ;
    cns:hasBenchmark cns:SHACL49CycleBenchmark .

# Weaver Engine Test Relationships
cns:WeaverEngine cns:hasBenchmark cns:WeaverBenchmark ;
    cns:hasBenchmark cns:Weaver80_20Benchmark .

# CNS Core Test Relationships
cns:CNSEngine cns:hasTest cns:CNSCoreTestSuite ;
    cns:hasTest cns:CNSCommandTests ;
    cns:hasTest cns:CNSDispatchTests ;
    cns:hasTest cns:CNSParserTests ;
    cns:hasTest cns:CNSTypeTests ;
    cns:hasBenchmark cns:ComprehensiveBenchmark ;
    cns:hasBenchmark cns:Real7TickBenchmark . 