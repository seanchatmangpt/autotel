  </owl:Class>
  <owl:Class rdf:about="#SprintBacklog">
    <rdfs:subClassOf rdf:resource="http://example.com/seon#Document"/>
  </owl:Class>
  <owl:Class rdf:about="#IntendedScrumDevelopmentTask">
