  </owl:Class>
  <owl:Class rdf:about="#ProductBacklogDefinition">
    <rdfs:subClassOf rdf:resource="http://example.com/seon#SpecificPerformedProjectProcess"/>
  </owl:Class>
  <owl:Class rdf:about="#Sprint">
    <rdfs:subClassOf rdf:resource="http://example.com/seon#SpecificPerformedProjectProcess"/>
  </owl:Class>
  <owl:Class rdf:about="#Ceremony">
    <rdfs:subClassOf rdf:resource="http://example.com/seon#PerformedProjectActivity"/>
  </owl:Class>
  <owl:Class rdf:about="#PlanningMeeting">
    <rdfs:subClassOf rdf:resource="#Ceremony"/>
  </owl:Class>
  <owl:Class rdf:about="#DailyStandupMeeting">
