  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#product_backlog_definition">
    <rdfs:domain rdf:resource="#ScrumProcess"/>
    <rdfs:range  rdf:resource="#ProductBacklogDefinition"/>
  </owl:ObjectProperty>

